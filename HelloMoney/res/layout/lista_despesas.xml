<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/textViewtipodespesa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lista de Despesas"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/listar_desp_orc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView" />

    <Button
        android:id="@+id/bt_l_vicios"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Vicios" />

    <Button
        android:id="@+id/bt_l_hab"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Habitação" />

    <Button
        android:id="@+id/bt_l_ali"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Alimentação" />

    <Button
        android:id="@+id/bt_l_viagens"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Viagens" />

    <Button
        android:id="@+id/bt_l_nao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Não Pré-Definida" />

    <Button
        android:id="@+id/bt_l_m_i"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Menu Inicial" />

</LinearLayout>
